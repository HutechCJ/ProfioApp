{
  "Spark": {
    "App": {
      "Name": "ENV_APP_NAME",
      "Environment": "ENV_ENVIRONMENT"
    },
    "Log": {
      "Default": "ENV_LOG_CHANNEL",
      "Level": "ENV_LOG_LEVEL",
      "Channels": {
        "File": {
          "Path": "Storage/Logging/Spark.log"
        },
        "Console": {
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Vite": {
    "Manifest": "assets.manifest.json",
    "Server": {
      "AutoRun": true,
      "Port": 5173
    }
  }
}
