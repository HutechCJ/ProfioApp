{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "profio-route": {
        "ClusterId": "profio-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "profio-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://profio.azurewebsites.net/"
          }
        }
      }
    }
  }
}
